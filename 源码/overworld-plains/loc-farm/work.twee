			<<set _stress to 10>>
			<<set _trauma to 0>>
		<<elseif $exhibitionism gte 35>>
			<<set _stress to 20>>
			<<set _trauma to 10>>
		<<else>>
			<<set _stress to 40>>
			<<set _trauma to 40>>
		<</if>>
		<<link [[Work anyway|Farm Work Naked]]>><<npcincr Alex lust 3>><<set $farm_naked to 1>><<stress _stress>><<trauma _trauma>><</link>><<gglust>>
		<<if _stress is 40>><<ggstress>><<else>><<gstress>><</if>>
		<<if _trauma is 40>><<ggtrauma>><<elseif _trauma is 5>><<gtrauma>><</if>>
		<br>
	<</if>>
	<<if $farm_work.alex isnot undefined and $farm_work.alex isnot "shower" and Time.hour gte 6 and Time.hour lte 20 and $slimeFarmNakedStored is undefined>>
		<<if $worn.upper.exposed gte 2 or $worn.lower.exposed gte 2>>
			<<link [[Ask Alex for clothes (0:05)|Farm Alex Clothes]]>><<pass 5>><<farm_yield -1>><<npcincr Alex dom 1>><<npcincr Alex lust 1>><</link>><<exhibitionist1>><<lfarm>><<gdom "Alex">><<glust>>
			<br>
		<<else>>
			<<link [[Ask Alex for something to dry with (0:05)|Farm Alex Dry]]>><<pass 5>><<farm_yield -1>><<npcincr Alex dom 1>><<npcincr Alex lust 1>><</link>><<exhibitionist1>><<lfarm>><<gdom "Alex">><<glust>>
			<br>
		<</if>>
	<</if>>
	<br>
	<<link [[Leave|Farmland]]>><</link>>
