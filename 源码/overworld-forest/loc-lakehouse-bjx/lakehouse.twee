:: Lake House Outdoor
<<set $outside to 1>><<set $location to "lakehouse">><<effects>>
/* <<run Furniture.in($location)>> */
<<lakehouseinit>>
<<if $lakehouse.build isnot undefined and $lakehouse.build isnot 0>>
	<<switch $lakehouse.build>>
	<<case "wall 1">>
		<span class="lblue">正在设立围栏地基。</span>
	<<case "wall 2">>
		<span class="lblue">一圈现代风格的栅栏正围着湖边小屋修建。</span>
	<<case "wall 3">>
		<span class="lblue">正在往栅栏上加装铁丝网。</span>
	<<case "tower 1">>
		<span class="lblue">正在建造着一座瞭望塔。</span>
	<<case "tower 2">>
		<span class="lblue">瞭望塔顶正在安装探照灯。</span>
	<<case "guard post">>
		<span class="lblue">岗哨正在修建中。</span>
	<<case "dog training">>
		<span class="lblue">驯兽设施正在建设中。</span>
	<<case "traps 1">>
		<span class="lblue">你的地产周围正在设置着陷阱。</span>
	<<case "traps 2">>
		<span class="lblue">你的地产周围正在设置着更多的陷阱。</span>
	<</switch>>
	<<if Time.hour gte 6>>
		<span class="blue">工人们在湖边来回穿梭。</span>
	<</if>>
	<<if $lakehouse.build_timer is 1>>
		估计将会在<span class="gold">明天</span>竣工。
	<<else>>
		估计将于<span class="gold"><<number $lakehouse.build_timer>></span>天后竣工。
	<</if>>
	<br><br>
<</if>>
<<if $lakehouse.billsphase gte 4>>
	你的门上贴着一张<span class="red">地产收回</span>的通知。
	<br><br>
<<elseif $lakehouse.billsphase gte 2>>
	你的门上贴着一张<span class="red">过期账单</span>的通知。
	<br><br>
<</if>>

<<if $stress gte $stressmax and !$possessed>>
	<<if $lakehouse.wall gte 2>>
		<<set $previousPassage to $passage>>
		<<passoutlakehouse>>
	<<else>>
		<<passoutlake>>
	<</if>>
<<else>>
	你在湖边小屋。 
	<<if Time.dayState is "night">>
		<<if $weather is "rain">>
			雨水从屋顶上滴下来。
		<<elseif $weather is "overcast">>
			在黑暗中你几乎什么也看不见。
		<<elseif $weather is "snow">>
			雪覆盖了道路。
		<<else>>
			星星于湖面上闪烁。
		<</if>>
	<<elseif Time.dayState is "dawn">>
		<<if $weather is "rain">>
			雨隐没在湖面中。
		<<elseif $weather is "overcast">>
			浓雾吞没了湖水。
		<<elseif $weather is "snow">>
			雪覆盖了湖面。
		<<else>>
			湖面迎着地平线上的朝阳。
		<</if>>
	<<elseif Time.dayState is "dusk">>
		<<if $weather is "rain">>
			雨水从海面吹来，笼罩着阴沉的天空。
		<<elseif $weather is "overcast">>
			已经很难看清湖了。
		<<elseif $weather is "snow">>
			雪飘入湖面。
		<<else>>
			太阳落在湖面上。
		<</if>>
	<<else>>
		<<if $weather is "rain">>
			雨水滋养着湖泊。
		<<elseif $weather is "overcast">>
			灰色的天空向四面八方延伸。
		<<elseif $weather is "snow">>
			雪覆盖了湖面。
		<<else>>
			阳光洒在波光粼粼的湖面上。
		<</if>>
	<</if>>
	<br><br>

	<<if $lakehouse.billsphase lt 4>>
		<<domusicon>><<link [[进入屋内 (0:01)|Lake House]]>><<set $phase to 1>><<pass 1>><</link>>
		<br>
	<</if>>
	<<link [[离开|Lake Shore]]>><</link>>
	<br>
<</if>>

:: Lake House
<<set $outside to 0>><<set $location to "lakehouse">><<effects>>
<<lakehouseinit>>
<<if ($lakehouse_countdown lte 0 or $lakehouse_countdown is undefined) and ($renttime lte -2 or ($renttime lte 0 and Time.hour gte 4 and Time.hour lte 21))>>
	<<unset $lakehouse_countdown>><<unset $baileyReunionScene>>
	<span class="red">你听到了敲门声。</span>
	<br><br>

	<<link [[应门|Lake House Bailey]]>><</link>>
	<br>
<!-- <<elseif $lakehouse_attack_timer lte 0 and Time.hour gte 21>> -->
<<elseif $lakehouse_bills>>
	<spawn class="red">你听见门外有人。</span>
	<br><br>
	
	<<link [[应门|Lake House Bills]]>><</link>>
	<br>
<<elseif $stress gte $stressmax and !$possessed>>
	<<set $previousPassage to $passage>>
	<<passoutlakehouse>>
<<else>>
	<<set _tv to Furniture.get('tv')>>
	<<set _gameconsole to Furniture.get('gameconsole')>>

	你进入了起居室。 
	<<if _tv>>
		你看见你的电视摆在起居室的桌子上，
		<<if _gameconsole>>
			旁边放着你的游戏机，
		<</if>>
	<</if>>
	
	<<if $lakehouse.petBed isnot undefined and $lakehouse.pet isnot undefined>>
		<<if $lakehouse.petBed gte 1>>
			角落里放着$lakehouse.petBed张看起来很柔软的宠物床，
			<<for _pet range $lakehouse.pet>>
				<<if _pet.type is "cat">>
					你的猫猫躺在里面睡觉，
				<<elseif _pet.type is "dog">>
					你的狗狗躺在里面睡觉，
				<</if>>
			<</for>>
		<</if>>
	<</if>>
	<br><br>

	<<bedroomicon>><<link [[去卧室|Lake House Bedroom]]>><</link>>
	<br>
	<<bathroomicon>><<link [[去浴室|Lake House Bathroom]]>><</link>>
	<br>
	<<gardenicon>><<link [[去后院|Lake House Backyard]]>><</link>>
	<br>
	<<if _tv>>
		<<if $lakehouse.billsphase gte 3>>
		没电的情况下你用不了电视。
		<br>
		<<else>>
			<<link [[看电视|Lake House TV]]>><</link>>
			<br>
		<</if>>
	<</if>>
	<<if _tv and _gameconsole>>
		<<if $lakehouse.billsphase gte 3>>
			没电的情况下你玩不了游戏机。
			<br>
		<<else>>
			<<link [[玩游戏(0:30)|Lake House Console]]>><<trauma -3>><<set $handskill += 1>><</link>><<ghandskill>>
			<br>
		<</if>>
	<</if>>
	<<if $lakehouse.pet isnot undefined>>
		<<for _pet range $lakehouse.pet>>
		    <<set $pet_now to _pet>>
			<<if _pet.type is "cat">>
        		<<link [[跟猫猫贴贴(0:30)|Lake House Snuggle Cat]]>><<pass 30>><<stress -10>><<trauma -10>><</link>><<llstress>><<lltrauma>>
        		<br>
        		<<link [[操猫猫|Lake House Fuck Cat]]>><<pass 30>><</link>><<deviant2>>
		        <br>
        	<<elseif _pet.type is "dog">>
        		<<link [[跟狗狗玩抛接 (1:00)|Lake House Fetch Dog]]>><<pass 30>><<stress -10>><<trauma -10>><<physique 5>><<tiredness 5>><</link>><<llstress>><<lltrauma>><<gphysique>><<gtiredness>>
        		<br>
        		<<link [[操狗狗|Lake House Fuck Dog]]>><<pass 30>><</link>><<deviant2>>
		        <br>
        	<</if>>
		<</for>>
	<</if>>
	/* <<link [[Robin options|Lake House Robin Options]]>><</link>>
	<br> */
	<<link [[离开|Lake House Outdoor]]>><</link>>
	<br>
<</if>>

:: Lake House Bedroom
<<lakehouseinit>>
<<bedclotheson "Lake House Bed">>
<<set $outside to 0>><<set $location to "lakehouse">><<effects>>
<<if $lakehouse.barrier is undefined>>
	<<getTarget true>>
<</if>>
<<if $stress gte $stressmax and !$possessed>>
	<<set $previousPassage to $passage>>
	<<passoutlakehouse>>
<<else>>
	你正待在你的卧室中。

	<<set _furniture to $furniture.bedroom>>
	<<if _furniture.wallpaper>>
		<<switch _furniture.wallpaper.name>>
			<<case "vines">>
				淫荡的绿色藤蔓在墙壁上蠕动着。
				<<if $backgroundTraits.includes("plantlover")>>
					对甜美花蜜的思念萦绕在你的脑海中。
				<</if>>
			<<case "tentacles">>
				紫色的触手在墙壁上滑动着。
				<<if $demon gte 6>>
					愉悦的念头闪过你的脑海。
				<</if>>
			<<case "plainwhite">>
				墙壁上贴着纯白色的壁纸。
			<<case "wavywhite">>
				白色壁纸以波浪形图案渐变为黑色，是有淫秽的暗示吗？
			<<case "hearts">>
				墙上布满着粉红色的爱心。
			<<case "cowgirls">>
				墙上画着跳舞的牛娘。
			<<case "trees">>
				墙上绿色的树木让你仿佛置身于森林。
			<<case "crosses">>
				金色的十字架覆盖着墙壁。
			<<default>> /* Custom */
				_furniture.wallpaper.name的图案覆盖着墙壁。
				<<if ['dol', 'degrees of lewdity'].some(e => _furniture.wallpaper.name === e)>>
					不知为何给你一种熟悉的感觉。
				<<elseif _furniture.wallpaper.name.startsWith('&lt;&lt')>>
					<span class="black">你的作弊行为被抓到了！</span>
				<</if>>
		<</switch>>
	<<else>>
		朴素的墙壁将你与外界隔绝开来。
	<</if>>

	<<set _deco to Furniture.get('decoration')>>
	<<if _deco>>
		<br>
		<<switch _deco.name>>
			<<case "large bear plushie">>
				<<furnitureicon _deco.iconFile>>_deco.nameCap被放在角落里。
			<<default>>
				<<furnitureicon _deco.iconFile>>_deco.nameCap正挂在墙上。
		<</switch>>
	<</if>>

	<<set _owlplush to Furniture.get('owlplushie')>>
	<<if _owlplush>>
		<br>
		<<switch _owlplush.id>>
			<<case "owlplushie">>
				<<furnitureicon _owlplush.iconFile>>_owlplush.nameCap与周围的黑暗融为一体，它貌似正在衣柜上凝视着你。
		<</switch>>
	<</if>>

	<<set _windowsill to Furniture.get('windowsill')>>
	<<if _windowsill>>
		<br>
		<<switch _windowsill.name>> /* can be expanded further */
			<<case "ominous jar">> /* unused */
				<<furnitureicon _windowsill.iconFile>>_windowsill.nameCap颇有气势地立在窗台上。
			<<default>>
				<<furnitureicon _windowsill.iconFile>>你的_windowsill.nameCap被放在了窗台上。
		<</switch>>
	<</if>>

	<<if _furniture.poster>>
		<br>
		<<switch _furniture.poster.name>>
			<<case "cowgirl">>
				墙上挂着一张牛娘海报。
			<<case "cat">>
				墙上挂着一张猫咪海报。下面的文字写着："坚持住！"
			<<case "puppy">>
				墙上挂着一张小狗的海报。
			<<default>> /* Custom */
				墙上挂着一张_furniture.poster.nameCap的海报。
				<<if ['dol', 'degrees of lewdity'].some(e => _furniture.poster.name === e)>>
					不知道为什么，你总感觉有一阵奇怪的熟悉感。
				<<elseif _furniture.poster.name.startsWith('&lt;&lt;')>>
					<span class="black">你的作弊行为被抓到了！</span>
				<</if>>
		<</switch>>
	<</if>>

	<br><br>
	/* <<if $robinbed is "yours" and Time.hour gte 6 and !$possessed>>
		<<unset $robinbed>>
		<<npc Robin>><<person1>>Robin yawns and heads to <<his>> own room.<<endevent>>
		<<if Time.hour is 6 and Time.minute lte 20>><<run setRobinLocationOverride("orphanage", 6)>><</if>>
		<br><br>
	<</if>> */
	<<if $unbind is 1>>
		<<unset $unbind>>
		<<if $possessed>>
			你看向身上那些拘束住你的东西。你只要轻轻一拉，它们就会松开，也就没办法再继续绑住你了。<<unbind>>
			<br><br>
		<<else>>
			你在桌子上摩擦你那些用于拘束你的东西。这需要一些努力，但是这些东西最终还是败在了你的努力之下，你的肢体终于得到解放。
			<br><br>
		<</if>>
	<</if>>
	<<if $nextPassageCheck is "Lake House Bedroom">>
		<span class="nextLink"><<link [[看向月亮|$passage]]>><</link>></span>
		<br>
	<</if>>
	<<if $stress gte $stressmax and !$possessed>>
		<<set $previousPassage to $passage>>
		<<passoutlakehouse>>
	/* <<elseif $christmas is 1 and Time.monthDay is 25 and $christmas_robin_visit is undefined and $christmas_robin_gift_received is undefined and Time.dayState isnot "night" and $NPCName[$NPCNameList.indexOf("Robin")].init is 1 and !$possessed>>
		<<set $christmas_robin_visit to true>>
		<<christmas_robin_visit>> */
	<<else>>
		<<if $leftarm is "bound" or $rightarm is "bound" or $feetuse is "bound">>
			<<link [[解开你的拘束 (0:10)|$passage]]>><<set $unbind to 1>><<pass 10>><<unbind>><</link>>
			<br><br>
		<</if>>
		/* <<if $robinpaid is 1 and $robinmissing is 0 and $robinnote isnot 1 and $NPCName[$NPCNameList.indexOf("Robin")].lust gte 10 and $NPCName[$NPCNameList.indexOf("Robin")].love gte 60 and $NPCName[$NPCNameList.indexOf("Robin")].trauma lt 10>>
			<span class="gold">There's a note by the window.</span>
			<br>
			<<link [[Read it|Robin Note]]>><<set $robinnote to 1>><</link>>
			<br><br>
		<</if>> */
		<<projectoptions>>
		<br>
		/* <<christmas_options>> */
		<<set _bed to Furniture.get('bed')>>

		<<if _bed.type.includes('comfy')>>
			你的床占据了这个房间的大部分空间。
