	| <<link ">>">><<set $money += 10000>><<clamp>><<replace "#statsmoney">><<print Math.trunc($money)>><</replace>><</link>>
	| <<link ">>>">><<set $money += 100000>><<clamp>><<replace "#statsmoney">><<print Math.trunc($money)>><</replace>><</link>>
	| <<link ">>>>">><<set $money += 1000000>><<clamp>><<replace "#statsmoney">><<print Math.trunc($money)>><</replace>><</link>>
	| <<link ">>>>>">><<set $money += 10000000>><<clamp>><<replace "#statsmoney">><<print Math.trunc($money)>><</replace>><</link>>
