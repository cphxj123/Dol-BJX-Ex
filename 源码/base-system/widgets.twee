			<<for $_ce range Object.keys($crime.events)>>
				<<set $_crDescLen to $crime.events[$_ce].filter(e => e.type is $_crimeType).length>>
				<<if $_crDescLen gte 1>>
					<<number $_crDescLen "silent">>
