					<<set $playerPregnancyBeastDisable to "f">>
					<<set $npcPregnancyDisable to "f">>
					<<set $incompletePregnancyDisable to "t">>
					<<set $cycledisable to "f">>
					<<set $pregnancytype to "realistic">>
